    "system_info" : {
        "CPU" : {
            "cmd" : {
                "armv7l" : "grep Hardware /proc/cpuinfo | awk -F ':' '{print $2}' | sed 's/ Sabre-SD Board//g'",
                "x86_64" : "cat /proc/cpuinfo | grep 'model name' | tail -n 1 | awk -F ':' '{print $2}' | xargs"
            },
            "compare" : {
                "armv7l" : "",
                "x86_64" : ""
            }
        },
        "Memery" : {
            "cmd" : {
                "armv7l" : "grep MemTotal /proc/meminfo | awk -F ':' '{print $2}' | awk -F ' ' '{print $1}'",
                "x86_64" : "grep MemTotal /proc/meminfo | awk -F ':' '{print $2}' | awk -F ' ' '{print $1}'"
            },
            "compare" : {
                "armv7l" : "",
                "x86_64" : ""
            }
        },
        "eMMC" : {
            "cmd" : {
                "armv7l" : "lsblk | grep 'mmcblk1 ' | awk '{print $4}'",
                "x86_64" : "fdisk -l | grep /dev/sda | awk -F ':' '{print $2}' | awk -F ',' '{print $1}' | head -n 1 | xargs"
            },
            "compare" : {
                "armv7l" : "",
                "x86_64" : ""
            }
        },
        "eMMC_Available" : {
            "cmd" : {
                "armv7l" : "df -h | grep /dev/root | awk -F ' ' '{print $4}'",
                "x86_64" : ""
            },
            "compare" : {
                "armv7l" : "",
                "x86_64" : ""
            }
        },
        "Operating_System_Type" : {
            "cmd" : {
                "armv7l" : "uname",
                "x86_64" : "uname"
            },
            "compare" : {
                "armv7l" : "",
                "x86_64" : ""
            }
        },
        "Linux_Version" : {
            "cmd" : {
                "armv7l" : "uname -a | awk -F ' ' '{print $3}'",
                "x86_64" : "uname -a | awk -F ' ' '{print $3}'"
            },
            "compare" : {
                "armv7l" : "",
                "x86_64" : "4.8.0-54-generic"
            }
        },
        "MAC1" : {
            "cmd" : {
                "armv7l" : "ifconfig | grep eth0 | awk -F ' ' '{print $5}'",
                "x86_64" : "uname -a | awk -F ' ' '{print $3}'"
            }
        },
        "Thermal" : {
            "cmd" : {
                "armv7l" : "cat /sys/class/thermal/thermal_zone0/temp",
                "x86_64" : "cat /sys/class/thermal/thermal_zone0/temp"
            }
        }
    },
    "network" : {
        "dhcp" : {
            "check_mode" : {
                "armv7l" : "grep 'iface eth0 inet dhcp' /etc/network/interfaces",
                "x86_64" : ""
            }
        },
        "static" : {
            "check_mode" : {
                "armv7l" : "grep 'iface eth0 inet static' /etc/network/interfaces",
                "x86_64" : ""
            },
            "ip" : {
                "armv7l" : "ifconfig 'eth0' | grep 'inet ' | awk -F ' ' '{print $2}' | awk -F ':' '{print $2}'",
                "x86_64" : ""
            },
            "netmask" : {
                "armv7l" : "ifconfig 'eth0' | grep 'inet ' | awk -F ' ' '{print $4}' | awk -F ':' '{print $2}'",
                "x86_64" : ""
            },
            "broadcast" : {
                "armv7l" : "ifconfig 'eth0' | grep 'inet ' | awk -F ' ' '{print $3}' | awk -F ':' '{print $2}'",
                "x86_64" : ""
            },
            "gateway" : {
                "armv7l" : "route -n | grep UG | head -n  1 | awk -F ' ' '{print $2}'",
                "x86_64" : "route -n | grep UG | head -n  1 | awk -F ' ' '{print $2}'"
            }
        },
        "ping" : {
            "ip" : "127.0.0.1"
        }
    },
    "date_and_time" : {
        "date" : {
            "armv7l" : "date '+%Y-%m-%d'",
            "x86_64" : "date '+%Y-%m-%d'"
        },
        "time" : {
            "armv7l" : "date '+%H:%M'",
            "x86_64" : "date '+%H:%M'"
        }
    },
    "hardware_test" : {
        "network1" : {
            "index" : 1,
            "shell" : "ping -c 1 -W 1 192.168.20.1 2>&1 | grep ', 0% packet loss'",
            "descriptor" : "eth0",
            "value" : "192.168.20.1",
            "status" : "error"
        },
        "Keyboard" : {
            "index" : 2,
            "shell" : "grep -v event0 /proc/bus/input/devices | grep 'kbd event'",
            "descriptor" : "usb",
            "status" : "error"
        },
        "Mouse" : {
            "index" : 3,
            "shell" : "grep -E 'Mouse|mouse|mice|Mice' /proc/bus/input/devices | grep 'N:'",
            "descriptor" : "usb",
            "status" : "error"
        },
        "MIO_USB" : {
            "index" : 4,
            "shell" : "mount | grep '/run/media/sda'",
            "descriptor" : "usb disk",
            "status" : "error"
        },
        "SDCard" : {
            "index" : 5,
            "shell" : "mount | grep '/run/media/mmcblk0'",
            "descriptor" : "sd card",
            "status" : "error"
        },
        "COM1" : {
            "index" : 6,
            "shell" : "mxc_uart_stress_test.out /dev/ttymxc1 115200 D D 1 10 O | grep ended",
            "descriptor" : "uart1",
            "status" : "error"
        },
        "I2C0" : {
            "index" : 7,
            "shell" : "i2cget -f -y 0 0x50 0x0 w | grep 0x",
            "descriptor" : "EEPROM(0X50) offset 0x0",
            "status" : "error"
        },
        "I2C1" : {
            "index" : 8,
            "shell" : "i2cget -f -y 1 0x32 0x10 w | grep 0x",
            "descriptor" : "RTC(0x32) offset 0x10",
            "status" : "error"
        },
        "Screen" : {
            "index" : 9,
            "shell" : "fbset -v | grep '/dev/fb0'",
            "descriptor" : "Framebuffer",
            "status" : "error"
        },
        "Touch" : {
            "index" : 10,
            "shell" : "cat /proc/bus/input/devices | grep 'ILITEK ILITEK-TP'  | awk '{print $2}'",
            "descriptor" : "USB ILITEK ILITEK-TP",
            "status" : "error"
        },
        "OTG" : {
            "index" : 11,
            "shell" : " lsusb | grep 'Bus 001' | grep -v root",
            "descriptor" : "USB OTG",
            "status" : "error"
        }
    },
    "extension_test" : {
        "COM3" : {
            "index" : 1,
            "shell" : "mxc_uart_stress_test.out /dev/ttymxc3 115200 D D 1 10 O | grep ended",
            "descriptor" : "uart3",
            "status" : "error"
        },
        "COM2" : {
            "index" : 2,
            "shell" : "mxc_uart_stress_test.out /dev/ttymxc2 115200 D D 1 10 O | grep ended",
            "descriptor" : "uart2",
            "status" : "error"
        },
        "GPIOS" : {
            "index" : 3,
            "shell" : "/usr/bin/gpiotest.sh | grep pass",
            "descriptor" : "GPIOS test",
            "status" : "error"
        }
    }
}
