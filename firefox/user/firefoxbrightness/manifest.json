{

  //"description": "Adds browser action icon to toolbar to open packaged web page. See https://developer.mozilla.org/en-US/Add-ons/WebExtensions/Examples#open-my-page-button",
  "name": "Brightness setup",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Send a message to a native application.",
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },

  "applications": {
    "gecko": {
      "id": "brightness@example.org",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "32.png"
  },

  "permissions": [
    "nativeMessaging"
  ]
}
